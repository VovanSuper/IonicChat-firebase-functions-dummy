{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../out-tsc/app",
    "paths": {
      "@env/*": [
        "environments/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@auth/*": [
        "app/auth/*"
      ],
      "@chats/*": [
        "app/chats/*"
      ],
      "@commons/*": [
        "app/shared/commons/*"
      ],
      "@services/*": [
        "app/shared/providers/*"
      ],
      "@validators/*": [
        "app/shared/helpers/validators/*"
      ],
      "@models/*": [
        "app/shared/helpers/models/*"
      ],
      "@auxiliary/*": [
        "app/shared/helpers/auxiliary/*"
      ],
      "@settings/*": [
        "app/shared/helpers/settings/*"
      ]
    }
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "./test.ts",
    "**/*.spec.ts"
  ]
}